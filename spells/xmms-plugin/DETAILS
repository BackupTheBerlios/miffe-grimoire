  VERSION=( "1.0" )
ATTRIBUTE="concurrent"
 CATEGORY="audio"
   OPT[0]="xmms-plugin,input-nsf:::For a plugin that plays nsf files"
   OPT[1]="xmms-plugin,input-ahx:::For a plugin that plays ahx/thx files"
   OPT[2]="xmms-plugin,input-sid:::For a plugin that plays sid files"
    SHORT="xmms-plugin Installs plugins for xmms"
stream << EOF
Included plugins for xmms:

 nsf - Plays nintendo sound files
 sid - Plays c64 audio
 ahx - Plays ahx/thx files
EOF
