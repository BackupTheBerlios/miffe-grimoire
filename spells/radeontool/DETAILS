     ATTRIBUTE="console new"
        SOURCE="software/radeon/radeontool-$VERSION"
           URL="http://fdd.com"
      HOMEPAGE="http://fdd.com/software/radeon"
           REQ="pciutils"
       #OPT[0]=":::"
